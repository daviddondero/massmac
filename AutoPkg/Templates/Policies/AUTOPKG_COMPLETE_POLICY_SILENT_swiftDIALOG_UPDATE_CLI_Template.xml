<?xml version="1.0" encoding="UTF-8"?>
<!-- SILENT INSTALL TEMPLATE -->
<policy>
   <general>
      <name>%POLICY_NAME_SILENT_UPDATE_CLI%</name>
      <enabled>false</enabled>
      <frequency>Once every day</frequency>
      <trigger_checkin>true</trigger_checkin>
      <category>
         <name>AUTOPKG SILENT_UPDATE_CLI</name>
      </category>
   </general>
   <scope>
      <computer_groups>
         <computer_group>
            <name>%GROUP_NAME%</name>
         </computer_group>
      </computer_groups>
   </scope>
   <scripts></scripts>
   <maintenance>
      <recon>true</recon>
   </maintenance>
   <files_processes>
      <run_command>jamf policy -event "install_swift%NAME%_AUTOPKG_MASTER"</run_command>
   </files_processes>
</policy>